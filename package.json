{
  "name": "react-app-starterkit",
  "version": "1.1.0",
  "description": "Starterkit for React - frostney edition",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "prebuild": "npm run clean && npm run copy",
    "build": "webpack --env=dist",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "karma start",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostney/react-app-starterkit.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starterkit",
    "app",
    "webapp"
  ],
  "author": "Johannes Stein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frostney/react-app-starterkit/issues"
  },
  "homepage": "https://github.com/frostney/react-app-starterkit#readme",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "exports-loader": "^0.6.3",
    "history": "^2.0.0",
    "react": "^0.14.7",
    "react-css-modules": "^3.7.0",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-istanbul-loader": "0.0.3",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "deep-extend": "^0.4.1",
    "enzyme": "^2.0.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "husky": "^0.11.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "mocha": "^2.4.5",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.4",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
