{
  "name": "react-app-starterkit",
  "version": "3.1.1",
  "description": "Starterkit for React - frostney edition",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "prebuild": "npm run clean && npm run copy",
    "build": "webpack --env=dist",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "BABEL_ENV=test ava --verbose 'src/**/*.test.js'",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "npm run serve",
    "precommit": "npm run lint",
    "deploy": "gh-pages -t -d dist -b gh-pages",
    "create-component": "babel-node ./scripts/create-component.js",
    "create-screen": "babel-node ./scripts/create-screen.js",
    "create-layout": "babel-node ./scripts/create-layout.js",
    "create-modifier": "babel-node ./scripts/create-modifier.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostney/react-app-starterkit.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starterkit",
    "app",
    "webapp"
  ],
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "ignore-styles"
    ]
  },
  "author": "Johannes Stein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frostney/react-app-starterkit/issues"
  },
  "homepage": "https://github.com/frostney/react-app-starterkit#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "history": "^3.0.0",
    "react": "^15.2.1",
    "react-css-modules": "^3.7.8",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-resolver": "0.0.7",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "bundle-loader": "^0.5.4",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.1",
    "deep-extend": "^0.4.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "husky": "^0.11.6",
    "ignore-styles": "^4.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "null-loader": "^0.1.1",
    "nyc": "^7.1.0",
    "open": "0.0.5",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
